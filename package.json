{
  "name": "react-ts-app",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Denis Makarov",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack-cli serve --color --env NODE_ENV=development --mode development --config webpack.client.js --progress",
    "build": "yarn && webpack --color --env NODE_ENV=production --mode production --config webpack.client.js",
    "build:storybook": "build-storybook",
    "test": "jest --config=jest.config.js",
    "analyzer": "yarn analyzer:create && yarn analyzer:open",
    "storybook": "start-storybook -p 6006",
    "analyzer:create": "webpack --env NODE_ENV=production --config webpack.client.js --profile --json > bundleSize.json",
    "analyzer:open": "webpack-bundle-analyzer bundleSize.json build/ -m server",
    "precommit": "lint-staged",
    "prettier:js": "prettier --config .prettierrc --write src/**/*.(ts|tsx|js|jsx)"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(ts|tsx)": [
      "./node_modules/.bin/eslint --config=.eslintrc",
      "./node_modules/.bin/prettier --config .prettierrc",
      "./node_modules/.bin/eslint --max-warnings=0"
    ],
    "src/**/*.scss": [
      "./node_modules/.bin/stylelint --config=.stylelintrc",
      "./node_modules/.bin/prettier --config .prettierrc",
      "./node_modules/.bin/stylelint --max-warnings=0"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 Edge version"
    ]
  },
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "axios": "^0.21.1",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "pure-react-carousel": "^1.27.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-lazyload": "^3.1.0",
    "react-reveal": "^1.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reactjs-popup": "^2.0.4",
    "swr": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-arrow-functions": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.14.0",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.14",
    "@testing-library/react": "^11.2.3",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/loadable__component": "^5.13.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-lazyload": "^3.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-loader": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.4.1",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.4",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "typescript-eslint": "^0.0.1-alpha.0",
    "webpack": "^5.15.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-font-preload-plugin": "^1.0.1",
    "webpack-node-externals": "^2.5.2"
  }
}
